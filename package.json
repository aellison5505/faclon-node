{
  "name": "falcon-node",
  "version": "0.3.2",
  "description": "Node addon for Falcon Post Quantum Signature",
  "main": "lib/index.js",
  "scripts": {
    "preinstall": "mkdir -p dep/falcon-1024/build && cd dep/falcon-1024/build && cmake .. && make ",
    "cmake": "npx cmake-js install",
    "cmake-config": "npx cmake-js configure",
    "cmake-compile": "npx cmake-js compile",
    "install": "npx cmake-js compile",
    "cmake-clean": "npx cmake-js clean && rm -rf dep/falcon-1024/build/*",
    "copyLib": "cp ./build/Release/falcon1024.node ./lib/",
    "test": "mocha",
    "watch": "tsc -w",
    "buildTS": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aellison5505/faclon-node.git"
  },
  "keywords": [
    "falcon",
    "cryptoPQ",
    "Post",
    "Quantum"
  ],
  "author": "Anthony Ellison",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aellison5505/faclon-node/issues"
  },
  "homepage": "https://github.com/aellison5505/faclon-node#readme",
  "devDependencies": {
    "@types/node": "^12.12.62",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.0.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "cmake-js": "^6.1.0",
    "node-addon-api": "^3.0.2"
  }
}
